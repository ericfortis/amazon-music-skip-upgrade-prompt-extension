{
  "name": "amazon-music-skip-upgrade-prompt-extension",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Prevents the upgrade prompt overlay",
  "background": {
    "service_worker": "service-worker.js"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "block_rule.json"
      }
    ]
  },
  "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback"]
}